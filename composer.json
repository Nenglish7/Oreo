{
    "name": "obozaxy/oreo",
    "type": "library",
    "keywords": [
        "php-library",
        "secure-by-default",
        "secure-coding",
        "fast",
        "eloquent",
        "session-management",
        "oreo",
        "php5-6",
        "php7-0",
        "php7-1",
        "php7-2"
    ],
    "description": "Secure by default, extremely fast, and eloquent session management that is designed to protect all your user sessions.",
    "homepage": "https://github.com/obozaxy/Oreo-Session-Management",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nicholas English",
            "homepage": "https://github.com/obozaxy",
            "email": "nenglish0820@outlook.com",
            "role": "Owner/Developer"
        }
    ],
    "require": {
        "php": "5.6|^7",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*"
    },
    "autoload": {
        "psr-4": {
            "Obozaxy\\Session\\": "./src"
        },
        "files": ["./session-boot.php"]
    },
    "require-dev": {
        "phpunit/phpunit": "^5|^6"
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
