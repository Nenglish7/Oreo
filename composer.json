{
    "name": "obozaxy/oreo",
    "type": "library",
    "keywords": [
        "php-library",
        "secure-by-default",
        "secure-coding",
        "fast",
        "eloquent",
        "session-management",
        "oreo",
        "php72"
    ],
    "description": "Secure by default, extremely fast, and eloquent session management.",
    "homepage": "https://github.com/obozaxy/oreo",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nicholas English",
            "homepage": "https://github.com/obozaxy",
            "email": "nenglish0820@outlook.com",
            "role": "Owner/Developer"
        }
    ],
    "require": {
        "php": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Obozaxy\\Session\\": "./src"
        },
        "files": ["./session-boot.php"]
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
